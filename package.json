{
  "name": "structures-cms",
  "version": "1.0.0",
  "description": "A customizable CMS made for my CS-546 final project",
  "main": "index.tsx",
  "scripts": {
    "start": "nodemon --watch 'src/server/**/*.ts' --exec 'ts-node' src/server/app.ts",
    "start:prod": "nodemon --watch 'src/server/**/*.ts' --exec NODE_ENV=prod 'ts-node' src/server/app.ts",
    "start:clean": "nodemon --watch 'src/server/**/*.ts' --exec CLEAN=true 'ts-node' src/server/app.ts",
    "start:prod:clean": "nodemon --watch 'src/server/**/*.ts' --exec NODE_ENV=prod CLEAN=true 'ts-node' src/server/app.ts",
    "build": "rimraf dist && webpack --config webpack/webpack.prod.js --bail",
    "clean": "rimraf dist"
  },
  "author": "Blake Zimmerman",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/blakezimmerman/structures-cms.git"
  },
  "dependencies": {
    "radium": "^0.19.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/cookie-parser": "^1.4.0",
    "@types/express": "^4.0.36",
    "@types/mongodb": "^2.2.9",
    "@types/node": "^8.0.20",
    "@types/radium": "^0.18.22",
    "@types/react": "^15.6.1",
    "@types/react-dom": "^15.5.2",
    "@types/react-redux": "^5.0.1",
    "@types/react-router": "^4.0.14",
    "@types/redux-logger": "^3.0.0",
    "@types/uuid": "^3.4.0",
    "@types/webpack-dev-middleware": "^1.9.2",
    "@types/webpack-hot-middleware": "^2.15.0",
    "awesome-typescript-loader": "^3.1.3",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.4",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.30.1",
    "mongodb": "^2.2.31",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1",
    "ts-node": "^3.3.0",
    "typescript": "^2.3.2",
    "uuid": "^3.1.0",
    "webpack": "^3.5.4",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0"
  }
}
